<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <LangVersion>latest</LangVersion>
        <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
        <RestoreProjectStyle>PackageReference</RestoreProjectStyle>
    </PropertyGroup>
    

    <ItemGroup>
      <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
      <PackageReference Include="ObjectDumper.NET" Version="3.0.20251.2-pre" />
    </ItemGroup>

    <ItemGroup>
      <None Update="MaterialIconKind.tt">
        <Generator>TextTemplatingFileGenerator</Generator>
        <LastGenOutput>MaterialIconKind.cs</LastGenOutput>
      </None>
      <None Update="MaterialIconDataFactory.tt">
        <Generator>TextTemplatingFileGenerator</Generator>
        <LastGenOutput>MaterialIconDataFactory.cs</LastGenOutput>
      </None>
    </ItemGroup>

    <ItemGroup>
      <Compile Update="MaterialIconKind.cs">
        <AutoGen>True</AutoGen>
        <DesignTime>True</DesignTime>
        <DependentUpon>MaterialIconKind.tt</DependentUpon>
      </Compile>
      <Compile Update="MaterialIconDataFactory.cs">
        <AutoGen>True</AutoGen>
        <DesignTime>True</DesignTime>
        <DependentUpon>MaterialIconDataFactory.tt</DependentUpon>
      </Compile>
    </ItemGroup>

</Project>
